{
    "$schema": "https://developer.microsoft.com/json-schemas/api-extractor/v7/api-extractor.schema.json",
    "mainEntryPointFilePath": "../../packages/react-sdk/dist/types/index.d.ts",
    "bundledPackages": [],
    "newlineKind": "lf",
    "enumMemberOrder": "preserve",

    "compiler": {
        "tsconfigFilePath": "../../packages/react-sdk/tsconfig.json",
        "overrideTsconfig": {},
        "skipLibCheck": true
    },

    "apiReport": {
        "enabled": false
    },

    "docModel": {
        "enabled": true,
        "apiJsonFilePath": "<projectFolder>/scripts/docgen/<unscopedPackageName>.api.json",
        "includeForgottenExports": true,
        "projectFolderUrl": "https://github.com/river-build/river/blob/main/packages/react-sdk"
    },

    "dtsRollup": {
        "enabled": false
    },

    "messages": {
        "extractorMessageReporting": {
            "default": { "logLevel": "warning" },
            "ae-different-release-tags": { "logLevel": "none" },
            "ae-incompatible-release-tags": { "logLevel": "none" },
            "ae-internal-mixed-release-tag": { "logLevel": "none" },
            "ae-missing-release-tag": { "logLevel": "none" },
            "ae-unresolved-link": { "logLevel": "warning" },
            "ae-forgotten-export": { "logLevel": "none" },
            "ae-wrong-input-file-type": { "logLevel": "none" }
        }
    }
}
