{
    "name": "@river-build/sdk",
    "version": "0.0.99",
    "packageManager": "yarn@3.8.0",
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "tsc",
        "cb": "yarn clean && yarn build",
        "clean": "rm -rf dist",
        "lint": "yarn eslint --format unix ./src  --max-warnings=0",
        "lint:fix": "yarn lint --fix",
        "test": "yarn test:ci:multi:ne",
        "test:ci": "yarn test:multi:ne",
        "test:multi:ent-legacy": "vitest --config vitest.config.entitlements-legacy.ts",
        "test:multi:ent": "vitest --config vitest.config.entitlements.ts",
        "test:multi:ne": "vitest",
        "test:ci:with-entitlements": "yarn test:ci:multi:ent",
        "watch": "tsc --watch"
    },
    "dependencies": {
        "@bufbuild/protobuf": "^1.9.0",
        "@connectrpc/connect": "^1.4.0",
        "@connectrpc/connect-web": "^1.4.0",
        "@ethereumjs/util": "^8.0.1",
        "@river-build/dlog": "workspace:^",
        "@river-build/encryption": "workspace:^",
        "@river-build/proto": "workspace:^",
        "@river-build/web3": "workspace:^",
        "browser-or-node": "^3.0.0",
        "debug": "^4.3.4",
        "dexie": "^4.0.7",
        "ethereum-cryptography": "^1.2.0",
        "ethers": "^5.7.2",
        "lodash": "^4.17.21",
        "nanoid": "^4.0.0",
        "p-limit": "^6.1.0"
    },
    "devDependencies": {
        "@testing-library/react": "^14.2.1",
        "@types/debug": "^4.1.8",
        "@types/lodash": "^4.14.186",
        "@types/node": "^20.5.0",
        "@types/seedrandom": "^3.0.4",
        "@typescript-eslint/eslint-plugin": "^6.10.0",
        "@typescript-eslint/parser": "^7.14.1",
        "eslint": "^8.53.0",
        "eslint-import-resolver-typescript": "^3.5.5",
        "eslint-plugin-import": "^2.27.5",
        "fake-indexeddb": "^4.0.1",
        "ioredis": "^5.3.2",
        "msgpackr": "^1.10.1",
        "seedrandom": "^3.0.5",
        "typed-emitter": "^2.1.0",
        "typescript": "^5.1.6",
        "vitest": "^2.1.4"
    },
    "files": [
        "/dist"
    ]
}
