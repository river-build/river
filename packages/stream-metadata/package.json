{
	"name": "@river-build/stream-metadata",
	"version": "0.0.0",
	"packageManager": "yarn@3.8.0",
	"private": true,
	"main": "dist/node.cjs",
	"type": "module",
	"scripts": {
		"compile": "node ./esbuild.config.mjs",
		"typecheck": "tsc --noEmit",
		"build": "yarn typecheck && yarn compile",
		"cp-config": "cp ./.env.local-sample ./.env.local && echo .env.local file copied successfully",
		"dev": "yarn dev:local_multi",
		"dev:local_multi": "yarn cp-config && RIVER_ENV=local_multi PORT=3002 RIVER_STREAM_METADATA_BASE_URL=http://localhost:3002 yarn run-build",
		"dev:local_multi_ne": "yarn cp-config && RIVER_ENV=local_multi_ne PORT=3003 RIVER_STREAM_METADATA_BASE_URL=http://localhost:3003 yarn run-build",
		"kill:local_multi": "kill -9 $(lsof -t -i:3002)",
		"kill:local_multi_ne": "kill -9 $(lsof -t -i:3003)",
		"lint": "eslint . --ext .ts",
		"lint:fix": "eslint . --ext .ts --fix",
		"prettier": "prettier --config ../prettier-config/config.js --ignore-path .prettierignore .",
		"run-build": "yarn build && node ./dist/node_esbuild.cjs",
		"start": "node ./dist/node_esbuild.cjs",
		"test": "vitest run",
		"test:integration": "vitest run --testNamePattern integration/stream-metadata",
		"test:watch": "vitest --watch"
	},
	"dependencies": {
		"@aws-sdk/client-cloudfront": "^3.645.0",
		"@bufbuild/buf": "^1.32.0",
		"@bufbuild/protobuf": "^1.9.0",
		"@connectrpc/connect": "^1.4.0",
		"@connectrpc/connect-node": "^1.4.0",
		"@fastify/cors": "^9.0.1",
		"@river-build/generated": "workspace:^",
		"@river-build/proto": "workspace:^",
		"@river-build/sdk": "workspace:^",
		"@river-build/web3": "workspace:^",
		"dd-trace": "^5.19.0",
		"dotenv": "^16.4.5",
		"esbuild-plugin-pino": "^2.2.0",
		"ethers": "^5.7.2",
		"fastify": "^4.28.1",
		"magic-bytes.js": "^1.10.0",
		"pino": "^8.17.1",
		"pino-pretty": "^10.2.3",
		"uuid": "^8.3.2",
		"zod": "^3.21.4"
	},
	"devDependencies": {
		"@river-build/dlog": "workspace:^",
		"@river-build/encryption": "workspace:^",
		"@river-build/eslint-config": "workspace:^",
		"@river-build/prettier-config": "workspace:^",
		"@types/node": "^20.5.0",
		"@types/uuid": "^10.0.0",
		"@typescript-eslint/parser": "^7.14.1",
		"axios": "^1.2.6",
		"esbuild": "^0.21.5",
		"eslint": "^8.53.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-import-resolver-typescript": "^3.5.5",
		"eslint-plugin-prettier": "^4.2.1",
		"fake-indexeddb": "^4.0.1",
		"prettier": "^2.8.8",
		"typescript": "^5.1.6",
		"vitest": "^2.1.4"
	}
}
